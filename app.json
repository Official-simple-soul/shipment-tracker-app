{
  "expo": {
    "name": "shipment-tracker-app",
    "slug": "shipment-tracker-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "developmentClient": {
      "silentLaunch": true
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.console_log.shipmenttrackerapp"
    },
    "androidStatusBar": {
      "backgroundColor": "#2F50C1",
      "translucent": false,
      "barStyle": "light-content"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "c6c3af07-137f-471f-b7dc-1d60c1939762"
      }
    },
    "owner": "console_log"
  }
}
